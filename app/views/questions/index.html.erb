

<h1>Questions</h1>



    <% @questions.each do |question| %>
        <h1><%= link_to question.title, question, :class => "text-body"%></h1>
        <% if user_signed_in? && current_user== question.user %>
        <%= link_to 'Edit', edit_question_path(question) %>
        <%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %>
        <% end %>

    <% end %>
    <%= line_chart Question.group_by_minute(:created_at).count %>
<br>
